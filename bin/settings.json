{
    "compile": {
        "jhtm": "compilerDefault",
        "jsd": "compilerShortdown"
    },
    "ignoreDir": [
        "style-guide"
    ],
    "ignoreFile": [
        "LICENSE.txt",
        "README.md",
        "SECURITY.md"
    ],
    "keepFiles": [
        "style-guide/lists.jhtm"
    ],
    "output": {
        "jhtm": "html",
        "jsd": "html"
    },
    "preprocessor": "less",
    "releaseDir": "release",
    "safeFileExtensions": [
        "7z",
        "css",
        "doc",
        "docx",
        "drawio",
        "gif",
        "htaccess",
        "htm",
        "html",
        "jpeg",
        "jpg",
        "js",
        "less",
        "md",
        "mp3",
        "mp4",
        "odt",
        "ogg",
        "pdf",
        "png",
        "ppt",
        "pptx",
        "svg",
        "ttf",
        "txt",
        "woff",
        "woff2",
        "xml",
        "zip"
    ]
}